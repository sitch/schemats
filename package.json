{
  "name": "@vramework/schemats",
  "version": "2.0.0",
  "description": "Generate typescript interface definitions from postgres SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "_type": "module",
  "_main": "./dist/index.js",
  "_types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "eslint:fix": "yarn run eslint --fix",
    "eslint": "node node_modules/eslint/bin/eslint \"{bin,src}/**/*.{ts,tsx}\"",
    "format:src": "prettier {bin,src} --write",
    "lint:fix": "yarn run eslint:fix",
    "lint:w": "run-p tscheck:w eslint:w",
    "mocha": "node -r @swc/register node_modules/mocha/bin/_mocha",
    "test:d": "ndb -r @swc/register node_modules/mocha/bin/_mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker run",
    "test:p": "ultra test",
    "test:r": "yarn run mocha -- src/**/*.spec.ts",
    "test:w": "yarn run test:r --watch-files src --watch",
    "test": "yarn run eslint && yarn run tscheck",
    "tscheck:w": "yarn run tscheck --watch",
    "tscheck": "tsc --noEmit",
    "up": "yarn upgrade-interactive --latest",
    "lint": "yarn run eslint && yarn run tscheck",
    "format": "yarn run format:src && yarn run lint:fix && yarn run tscheck"
  },
  "bin": {
    "schemats": "dist/bin/schemats.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vramework/schemats.git"
  },
  "bugs": {
    "url": "https://github.com/vramework/schemats/issues"
  },
  "author": "Vlandor Ltd",
  "contributors": [
    "Mengxuan Xia <xiamx2004@gmail.com>",
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^5.6.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.168",
    "@swc/helpers": "^0.3.8",
    "@swc/jest": "^0.2.20",
    "@swc/register": "^0.1.10",
    "@tsconfig/recommended": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inflection": "^1.13.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.25",
    "@types/pg": "^8.6.5",
    "@types/sinon": "^10.0.11",
    "@types/sort-json": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "expect": "^27.5.1",
    "jest": "27.5.1",
    "jest-cli": "27.5.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "simplytyped": "^3.3.0",
    "ts-node": "^10.6.0",
    "tsconfig-paths-jest-mapper": "^1.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "ultra-runner": "^3.10.5"
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "chalk": "4.1.2",
    "commander": "^9.2.0",
    "inflection": "^1.13.2",
    "json5": "^2.2.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3"
  },
  "_peerDependencies": {
    "mysql2": ">= 2",
    "pg": ">= 8"
  },
  "_peerDependenciesMeta": {
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  }
}
